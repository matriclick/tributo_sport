/*------------------- Industry Categories CSS --------------------*/

.box_industry_categories {
	float:left;
	width:120px;
	height:134px;
	font-size:11px;
	text-align:center;
	margin:0px 40px 20px 0;
}
.box_industry_categories_home {
	float:left;
	width:120px;
	height:134px;
	font-size:11px;
	text-align:center;
	margin:0px 20px 5px 20px;
}
.image_industry_categories {
	position:relative;
	width:120px;
	height:100px;
	margin-bottom:5px;
}
.discount {
	position: absolute;
    left: 0px;
    top: 0px;
}
.right_top_box_logo {
	position: absolute;
    right: 0px;
    top: 0px;
}
.right_bottom_box_logo {
	position: absolute;
    right: 0px;
    bottom: 0px;
}
@charset "UTF-8";
/* CSS Document */

/* ------------------------ Product In CSS ------------------------ */
.supplier_page_in_image {
	width:200px;
	border:10px solid #eee;
	float:left;
	margin-right:20px;
	text-align: center;
	display:table-cell;
	vertical-align:middle;
}
.margin_left_30 {
	margin-left:30px;
}
.supplier_page_in_image_count {
	color:#bbb;
	height:40px;
	margin-bottom:5px;
	overflow:hidden;
	border-top:1px dotted #ddd;
}
.supplier_page_in_image_count img{
	position:relative;
	top:4px;
}
.supplier_page_in_data {
	max-width: 388px;
	position:relative;
	float:left;
}
.supplier_page_in_name {
	color:#444;
	font-size:25px;
	font-weight:bolder;
}	
ul.supplier_page_in_supplier {
	color:#444;
	font-size:12px;
}
li.supplier_data {
	margin-top:3px;
}
.supplier_page_in_price	{
	font-size:14px;
	color:#444;
	margin-top:10px;
}
.supplier_page_in_stars	{
	
}
.supplier_page_in_links {

}
.supplier_page_in_menu {
	float:left;
	width:77%;
	font-size:12px;
	margin-top:25px;
}
.supplier_page_in_menu a {
	text-decoration:none;
}
.supplier_page_in_menu li:hover {
	background-color: #dedede;
}
.supplier_page_in_content {
	float:left;
	width:72%;
	padding:10px;
	border:solid 1px #ccc;
	margin-top:-1px;
	background:#FFF;
}
.supplier_page_in_content_dress {
	float:left;
	width:99%;
	padding:10px;
	border:solid 1px #ccc;
	margin-top:-1px;
	background:#FFF;
}

.arrows_image_data_wrapper {
	float:left;
	width:77%;
	position:relative;
	min-height:190px;
}
.supplier_page_star_rating{
	width: 30%;
}

.star_rating_content {
	width:95%;
}

.supplier_page_rating_content{
	width:60%;
	float:left;
}

.rating_content{
	float:right;
}

/*
* This is the feedback tab css
*/
.slide-out-div {
  padding: 20px;
  width: 250px;
  height: 250px; /* the real height is this plus 42px */
  background: #eee;
  border: 1px solid #ccc;
  z-index:100;
}
.slide-out-div a {
	height:25px;
	width:220px;
	background:url(<%= asset_path("feedback.png") %>) no-repeat;
}
.feedback_field {
	width:97%;
}
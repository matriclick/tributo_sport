<div class="titulo_style">
	<h1><%= t "Listing product_types" %></h1>
</div>

<table>
	<tr>
		<th>Industry category</th>
		<th>Name</th>
		<th>Opciones</th>
	</tr>

	<% @product_types.each do |product_type| %>
	<tr>
		<td><%= product_type.industry_category.get_name %></td>
		<td><%= product_type.name %></td>
		<td><%= link_to 'Show', product_type %></td>
		<td><%= link_to 'Edit', edit_product_type_path(product_type) %></td>
		<td><%= link_to 'Destroy', product_type, confirm: 'Are you sure?', method: :delete %></td>
	</tr>
	<% end %>
</table>

<br />
<br />

<%= link_to 'New Product type', new_product_type_path %>

<div class="right_link">
	<%= link_to t('new'), new_supplier_account_event_path(@supplier), :class => :button %>
</div>
<div class="titulos">
	<%= t "listing_events" %>
</div>
<table>
	<tr>
		<th><%= t 'activerecord.attributes.event.title' %></th>
		<th><%= t 'activerecord.attributes.event.service_id' %></th>
		<th><%= t 'activerecord.attributes.event.starts_at' %></th>
		<th><%= t 'activerecord.attributes.event.description' %></th>
	</tr>
	<% @events.each do |event| %>
	  <tr>
		  <td> <%= event.title %> </td>
		  <td> <%= event.email %> </td>
		  <td> <%= event.service_id %> </td>
		  <td> <%= event.description %> </td>
	  </tr>
	<% end %>
</table>
<br />
